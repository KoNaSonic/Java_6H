{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ParentAndChild": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/ParentAndChild",
      "sourceRoot": "projects/ParentAndChild/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/ParentAndChild",
            "index": "projects/ParentAndChild/src/index.html",
            "main": "projects/ParentAndChild/src/main.ts",
            "polyfills": "projects/ParentAndChild/src/polyfills.ts",
            "tsConfig": "projects/ParentAndChild/tsconfig.app.json",
            "aot": true,
            "assets": [
              "projects/ParentAndChild/src/favicon.ico",
              "projects/ParentAndChild/src/assets"
            ],
            "styles": [
              "projects/ParentAndChild/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "projects/ParentAndChild/src/environments/environment.ts",
                  "with": "projects/ParentAndChild/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ParentAndChild:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ParentAndChild:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ParentAndChild:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/ParentAndChild/src/test.ts",
            "polyfills": "projects/ParentAndChild/src/polyfills.ts",
            "tsConfig": "projects/ParentAndChild/tsconfig.spec.json",
            "karmaConfig": "projects/ParentAndChild/karma.conf.js",
            "assets": [
              "projects/ParentAndChild/src/favicon.ico",
              "projects/ParentAndChild/src/assets"
            ],
            "styles": [
              "projects/ParentAndChild/src/styles.css"
            ],
            "scripts": []
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/ParentAndChild/tsconfig.app.json",
              "projects/ParentAndChild/tsconfig.spec.json",
              "projects/ParentAndChild/e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "projects/ParentAndChild/e2e/protractor.conf.js",
            "devServerTarget": "ParentAndChild:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ParentAndChild:serve:production"
            }
          }
        }
      }
    },
